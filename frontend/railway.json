{
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "echo 'UI build complete'"
  },
  "deploy": {
    "startCommand": "python -m http.server 8080 --directory ui",
    "healthcheckPath": "/",
    "healthcheckTimeout": 300,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}